{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}

{% set icon %}
<img width="29" height="25" alt="Redis" style="border-width: 0; vertical-align: middle; margin-right: 5px;" src="data:image/png;base64,"/>
{% endset %}
{% set text %}
    <div class="sf-toolbar-info-piece">
        <b>Execution time</b>
        <span>{{ '%0.2f'|format(collector.executionTime * 1000) }} ms </span>
    </div>
    <div class="sf-toolbar-info-piece">
        <b>Commands</b>
        <span>{{ collector.commandsCount }}</span>
    </div>
{% endset %}
{% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon"><img alt="Redis" src="data:image/png;base64,"/></span>
    <strong>Redis</strong>
    <span class="count">
        <span>{{ collector.commandsCount }}</span>
        <span>{{ '%0.2f'|format(collector.executionTime * 1000) }} ms</span>
    </span>
</span>
{% endblock %}

{% block panel %}
<h2>Commands</h2>

{% if collector.commandsCount == 0 %}
<p><em>No redis commands executed.</em></p>
{% else %}
<ul class="alt">
    {% for command in collector.commands %}
    <li class="{{ cycle(['odd', 'even'], loop.index) }}{% if command.error %} error{% endif %}">
        <div>
            <code>{{ command.command }}</code>
        </div>
        <small>
            <strong>Time</strong>: {{ '%0.2f'| format(command.executionTime * 1000) }} ms
            <strong>Connection</strong>: {{ command.connection }}
            {% if command.error %}<strong>Error</strong>: {{ command.error }}{% endif %}
        </small>
    </li>
    {% endfor %}
</ul>


<h2>Commands repetitions</h2>
<ul class="alt">
    {% for command, count in collector.repetitions %}
    <li class="{{ cycle(['odd', 'even'], loop.index) }}">
        <div>
            <code>{{ command }}</code>
        </div>
        <small>
            <strong>{{ count }}</strong>

        </small>
    </li>
    {% endfor %}



</ul>

{% endif %}

{% endblock %}